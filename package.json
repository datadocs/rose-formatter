{
  "description": "Process number format information specified as text",
  "name": "@datadocs/formatter",
  "license": "MIT",
  "author": "Alexandre Felipe <o.alexandre.felipe@gmail.com>",
  "version": "0.0.2",
  "private": false,
  "main": "dist/index.js",
  "files": [
    "dist/index.js",
    "src/index.d.ts"
  ],
  "scripts": {
    "generate": "peggy --format umd src/parse-format.peggy",
    "generate2": "peggy --format umd src/parse-format2.peggy",
    "build": "npm run generate && npm run generate2 && npx tsc -d && npx webpack",
    "test": "npm run build && jest",
    "cpp-scanner": "node  ./cpp-plugin/cli.js ./src/parse-format2.peggy src/cpp/scanner.cpp"
  },
  "devDependencies": {
    "@webpack-cli/generators": "^2.4.1",
    "jest": "^27.0.4",
    "peggy": "^1.2.0",
    "webpack-cli": "^4.9.1",
    "typescript": "^4.4.2",
    "canvas": "^2.9.3",
    "gif-encoder-2": "^1.0.5",
    "upng-js": "^2.1.0"
  },
  "dependencies": {
  }
}
